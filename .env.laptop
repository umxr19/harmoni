# Laptop-specific environment settings
NODE_ENV=development
PORT=3000

# Use actual databases on the laptop for full functionality
USE_IN_MEMORY=false
MONGODB_URI=mongodb://localhost:27017/harmoni
REDIS_URL=redis://localhost:6379

# API Configuration
JWT_SECRET=your-jwt-secret-for-laptop
JWT_EXPIRES_IN=24h

# Use real data on the laptop for backend development
USE_MOCK_DATA=false

# OpenAI (if used) - copy from your main .env
OPENAI_API_KEY=sk-project-placeholder

# Stripe (if used) - use test keys for development
STRIPE_PUBLIC_KEY=pk_test_placeholder
STRIPE_SECRET_KEY=sk_test_placeholder 