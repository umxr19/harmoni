# Mac-specific environment settings
NODE_ENV=development
PORT=3000

# Using in-memory mode for lighter development on Mac
# No need for MongoDB or Redis on the Mac
USE_IN_MEMORY=true

# API Configuration
JWT_SECRET=your-jwt-secret-for-mac
JWT_EXPIRES_IN=24h

# Mock data for UI development
USE_MOCK_DATA=true

# OpenAI (if used) - copy from your main .env
OPENAI_API_KEY=sk-project-placeholder

# Stripe (if used) - use test keys for development
STRIPE_PUBLIC_KEY=pk_test_placeholder
STRIPE_SECRET_KEY=sk_test_placeholder 