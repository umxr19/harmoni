NODE_ENV=staging
MONGODB_URI=mongodb+srv://your_staging_user:your_staging_password@your-staging-cluster.mongodb.net/question-bank-staging?retryWrites=true&w=majority
JWT_SECRET=your_secure_staging_jwt_secret
JWT_EXPIRES_IN=24h

# Email configuration
SMTP_USER=staging-noreply@yourdomain.com
SMTP_PASS=your_secure_smtp_password
SMTP_FROM=staging-noreply@yourdomain.com
FRONTEND_URL=https://staging.your-domain.com

PORT=8080
REACT_APP_API_URL=https://staging-api.your-domain.com/api

# Stripe configuration (test keys)
STRIPE_SECRET_KEY=sk_test_your_stripe_test_secret_key
STRIPE_WEBHOOK_SECRET=whsec_your_stripe_test_webhook_secret
STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_test_publishable_key

# Security settings
SECURE_COOKIES=true
CORS_ORIGIN=https://staging.your-domain.com
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX=200

# Logging
LOG_LEVEL=debug 